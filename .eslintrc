{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }, 
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-indent": [2, 2],
    "arrow-body-style": ["error", "always"]
  }
}